# python
alias killsomething='cmd //c "taskkill /F /IM $1 /T"'
alias killpython='cmd //c "taskkill /F /IM python.exe /T"'
alias rmvenv='mkdir emptyTmpFolder && robocopy emptyTmpFolder venv //MIR && rm -rf emptyTmpFolder && rm -rf venv'
