--langdef=markdown
--langmap=markdown:.md.markdown.mdown.mkd.mkdn
--regex-markdown=/^#[ \t]+(.*)/\1/h,heading1/
--regex-markdown=/^##[ \t]+(.*)/\1/h,heading2/
--regex-markdown=/^###[ \t]+(.*)/\1/h,heading3/

--python-kinds=cfm
--recurse
--exclude=build
--exclude=dist
--exclude=Python34
--exclude=webclient
--exclude=node_modules
--exclude=package
--exclude=.eggs
